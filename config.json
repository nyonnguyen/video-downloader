{
  "browser": "chromium",
  "timeout": 60,
  "apps": [
    {
      "name": "ixigua",
      "base_url": "https://www.ixigua.com",
      "fetching_pattern": ".ixigua.com/video",
      "video_url_pattern": "(.*?)\\?",
      "title_locator": "//div[contains(@class, 'videoTitle')]",
      "video_locator": "video",
      "resolution": "P1080"
    },
    {
      "name": "youtube",
      "base_url": "https://www.youtube.com",
      "resolution": "HIGH_1080P"
    },
    {
      "name": "douyin",
      "base_url": "https://www.douyin.com",
      "fetching_pattern": "mime_type=video_mp4",
      "video_url_pattern": "mime_type=video_mp4",
      "title_locator": "h1",
      "video_locator": "video",
      "is_audio": true,
      "resolution": "AUTO"
    }
  ]
}